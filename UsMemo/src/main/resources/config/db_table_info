<테이블 정보>

create table member(
	id varchar(100) primary key,
	nickname varchar(100),
	name varchar(100),
    email varchar(100),
    profile_image varchar(500)
);

create table board(
	num number primary key,
	name varchar(100)
);

create table list(
	num number primary key,
	name varchar(100),
	bNum number,
	llink number,
	rlink number
);

create table card(
	num number primary key,
	name varchar(200),
	content CLOB,
	attach BLOB,
	memId varchar(100),
	n_date date,
	lNum number,
	llink number,
	rlink number
);

create table reply(
	num number primary key,
	content CLOB,
	memId varchar(100),
	n_date date,
	cNum number,
	attach BLOB
);

create table activity(
	num number primary key,
	activity_num number not null,
	memId varchar(100),
	bNum number,
	write_time date,
	value_num number,
	value_string varchar(200),
	to_num number,
	from_num number
);

create table activity_format (
	num number primary key,
	activity_name varchar2(80),
	format varchar2(4000)
);

create table memberGrade(
	num number primary key,
	bNum number,
	memId varchar(100),
	grade number,
    star varchar(2)
);

create sequence board_num_seq start with 1; 
create sequence list_num_seq start with 1;
create sequence card_num_seq start with 1;
create sequence memberGrade_num_seq start with 1; 
create sequence activity_format_num_seq start with 1; 
create sequence activity_num_seq start with 1; 
