<테이블 정보>

create table member(
	id varchar(100) primary key,
	nickname varchar(100),
	name varchar(100),
    email varchar(100),
    profile_image varchar(500)
);

create table board(
	num number primary key,
	name varchar(100)
);

create table list(
	num number primary key,
	name varchar(100),
	bNum number,
	location number
);

create table card(
	num number primary key,
	name varchar(200),
	content CLOB,
	attach BLOB,
	memId varchar(100),
	n_date date,
	lNum number,
	location number
);

create table reply(
	num number primary key,
	content CLOB,
	memId varchar(100),
	n_date date,
	cNum number,
	attach BLOB
);

create table activity(
	num number primary key
);

create table memberGrade(
	num number primary key,
	bNum number,
	memId varchar(100),
	grade number

);

create sequence board_num_seq start with 1; 
create sequence list_num_seq start with 1;
create sequence card_num_seq start with 1;
create sequence memberGrade_num_seq start with 1; 
